  "description": "Egeo is an angular common components library",
  "author": "Stratio",
  "scripts": {
    "start": "ng serve",
    "release": "gulp publish",
    "clean": "gulp clean",
    "build": "gulp aot:build",
    "build:web:aot": "gulp aot:build",
    "build:lib": "gulp egeo:build-release:clean",
    "build:theme": "gulp build:styles",
    "build:demo-app": "npm run clean && npm run demo-app:create-dist && npm run demo-app:generate",
    "demo-app:generate": "ng build --prod --aot=false --progress=false && gulp demo-app:replace:assets",
    "demo-app:create-dist": "gulp demo-app:create-dist",
    "test": "karma start test/karma.conf.js",
    "test:watch": "karma start test/karma.conf.js --auto-watch --no-single-run",
    "lint": "gulp lint",
    "replace": "json -I -f package.json -e",
    "commit": "git-cz",
    "serve:coverage": "http-server target/html",
    "serve:build": "npm run build:demo-ap && http-server dist/demo-app"
  },
  "repository": {
    "type": "git",
