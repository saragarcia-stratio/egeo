{
  "name": "egeo",
  "version": "",
  "description": "Egeo Library. Common components, styles, fonts and website",
  "author": "Stratio",
  "scripts": {
    "clean": "npm run clean:dist && npm run clean:doc && npm run clean:coverage && npm run clean:temp",
    "clean:dist": "rimraf dist",
    "clean:aot": "rimraf temp/compiled",
    "clean:coverage": "rimraf target/coverage",
    "clean:doc": "rimraf target/site/web",
    "clean:dll": "rimraf temp/dll",
    "clean:temp": "rimraf temp",
    "build": "npm run build:doc && npm run build:lib",
    "build:doc": "npm run clean:aot && npm run clean:doc && npm run webpack:doc:aot",
    "build:lib": "npm run clean:dist && npm run gulp && npm run ngc && npm run webpack:lib",
    "webpack:doc:aot": "webpack --config config/webpack/webpack.prod.js  --no-color --bail",
    "webpack:lib": "webpack --config config/webpack-components.js  --no-color --bail",
    "lint": "tslint -c ./tslint.json -e 'node_modules' --type-check -t stylish '{src,components}/**/*.ts'",
    "replace": "json -I -f package.json -e",
    "start:dev:hmr": "npm run start -- --hot --inline",
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --profile --open --watch --content-base ./",
    "server:prod": "http-server target/site/web -c-1 --cors",
    "test": "cross-env NODE_ENV=test karma start",
    "test:watch": "cross-env NODE_ENV=test karma start --auto-watch --no-single-run",
    "gulp": "gulp",
    "ngc": "ngc -p tsconfig.gulp.json"
  },
  "main": "dist/bundle/egeo.js",
  "typings": "dist/components/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Stratio/egeo"
  },
  "license": "Apache-2.0",
